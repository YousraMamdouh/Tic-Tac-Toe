<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tictactoeclient.FXMLController">
   <children>
      <ImageView fitHeight="727.0" fitWidth="993.0" layoutX="-19.0" layoutY="-12.0" pickOnBounds="true" preserveRatio="true" style="-fx-cursor: copy;">
         <image>
            <Image url="@login_signup.png" />
         </image>
         <effect>
            <Glow />
         </effect>
      </ImageView>
      <Button fx:id="loginPageButton" layoutX="402.0" layoutY="417.0" mnemonicParsing="false" onAction="#loggingIn" prefHeight="48.0" prefWidth="151.0" stylesheets="@HomePageStyle.css" text="login" textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="20.0" />
         </font>
      </Button>
      <Button fx:id="passButton" layoutX="147.0" layoutY="287.0" mnemonicParsing="false" prefHeight="44.0" prefWidth="157.0" stylesheets="@HomePageStyle.css" text="Password:" textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="20.0" />
         </font>
      </Button>
      <TextField fx:id="emailTextField" layoutX="337.0" layoutY="234.0" prefHeight="43.0" prefWidth="424.0" styleClass="textField" stylesheets="@HomePageStyle.css" />
      <TextField fx:id="passTextField" layoutX="337.0" layoutY="288.0" prefHeight="43.0" prefWidth="424.0" styleClass="textField" stylesheets="@HomePageStyle.css" />
      <Label layoutX="474.0" layoutY="93.0" prefHeight="53.0" prefWidth="104.0" text="login" textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="36.0" />
         </font>
      </Label>
      <Button fx:id="emailButton" layoutX="147.0" layoutY="233.0" mnemonicParsing="false" prefHeight="43.0" prefWidth="157.0" stylesheets="@HomePageStyle.css" text="Email:" textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
