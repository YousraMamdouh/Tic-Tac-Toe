<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="tictactoeclient.OnlineBoardController"
            prefHeight="400.0" prefWidth="600.0">

    <children>
        <ImageView fitHeight="450.0" fitWidth="715.0" layoutX="-3.0" layoutY="-1.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../res/Result.png" />
            </image>
        </ImageView>
        <Label fx:id="winnerLabel" alignment="CENTER" layoutX="194.0" layoutY="74.0" prefHeight="35.0" prefWidth="317.0" style="-fx-font-size: 36;" styleClass="title" stylesheets="@../res/ClientStyles.css" text="#Player Wins!" textAlignment="CENTER" textFill="WHITE">
            <font>
                <Font name="Comic Sans MS Bold" size="24.0" />
            </font>
        </Label>
        <Label alignment="CENTER" layoutX="182.0" layoutY="204.0" prefHeight="154.0" prefWidth="325.0" text="Awesome Video Here!">
            <font>
                <Font size="28.0" />
            </font>
        </Label>
        <Button layoutX="282.0" layoutY="385.0" mnemonicParsing="false" onAction="#exitGame" prefHeight="41.0" prefWidth="140.0" style="-fx-font-size: 18;" styleClass="myButtonPink" stylesheets="@../res/ClientStyles.css" text="OK" textFill="WHITE">
            <font>
                <Font name="Comic Sans MS Bold" size="19.0" />
            </font>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>
    </children>
</AnchorPane>
