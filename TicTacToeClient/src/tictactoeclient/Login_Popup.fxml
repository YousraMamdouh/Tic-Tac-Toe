      </Label>
      <Button fx:id="emailButton" layoutX="147.0" layoutY="234.0" mnemonicParsing="false" prefHeight="43.0" prefWidth="157.0" style="-fx-font-size: 18;" styleClass="label" stylesheets="@../res/ClientStyles.css" text="Email:" textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="20.0" />
         </font>
      </Button>
      <Button fx:id="cancelButton" focusTraversable="false" layoutX="468.0" layoutY="423.0" mnemonicParsing="false" onAction="#cancelAction" prefHeight="44.0" prefWidth="143.0" styleClass="myButtonPink" stylesheets="@../res/ClientStyles.css" text="Cancel" />
      <PasswordField fx:id="passField" layoutX="361.0" layoutY="293.0" prefHeight="43.0" prefWidth="424.0" styleClass="roundedTextField" stylesheets="@../res/ClientStyles.css" />
