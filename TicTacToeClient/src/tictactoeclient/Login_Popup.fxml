      </ImageView>
      <Button fx:id="loginPageButton" layoutX="226.0" layoutY="423.0" mnemonicParsing="false" onAction="#loggingIn" prefHeight="51.0" prefWidth="143.0" styleClass="myButtonPink" stylesheets="@../res/ClientStyles.css" text="login" textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="20.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="passButton" layoutX="147.0" layoutY="288.0" mnemonicParsing="false" prefHeight="44.0" prefWidth="157.0" style="-fx-font-size: 18;" styleClass="label" stylesheets="@../res/ClientStyles.css" text="Password:" textFill="WHITE">
