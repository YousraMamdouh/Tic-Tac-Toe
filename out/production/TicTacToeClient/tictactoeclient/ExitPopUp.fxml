<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="417.0" prefWidth="660.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tictactoeclient.GameBoardController">
   <children>
      <ImageView fitHeight="450.0" fitWidth="660.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../res/Exit/popUp.png" />
         </image>
      </ImageView>
      <Label alignment="CENTER" layoutX="26.0" layoutY="258.0" prefHeight="55.0" prefWidth="609.0" text="Are you sure you want to leave ?" textFill="WHITE">
         <font>
            <Font name="Comic Sans MS Bold" size="31.0" />
         </font>
      </Label>
      <Button fx:id="yesButton" layoutX="97.0" layoutY="346.0" mnemonicParsing="false" onAction="#setYesButton" prefHeight="48.0" prefWidth="166.0" style="-fx-background-color: linear-gradient(to right,#d53772,#8653e0);        -fx-background-radius: 10px;;" text="Yes" textFill="WHITE">
         <font>
            <Font name="Comic Sans MS Bold" size="22.0" />
         </font>
      </Button>
      <Button fx:id="noButton" layoutX="383.0" layoutY="346.0" mnemonicParsing="false" onAction="#setNoButton" prefHeight="48.0" prefWidth="166.0" style="-fx-background-color: linear-gradient(to right,#d53772,#8653e0);        -fx-background-radius: 10px;;" text="No" textFill="WHITE">
         <font>
            <Font name="Comic Sans MS Bold" size="22.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
